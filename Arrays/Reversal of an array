#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int t[n];int m=0;
    for(int i=n-1;i>=0;i--){
        t[m]=a[i];
        m++;
    }
    cout<<"new array: ";
    for(int i=0;i<n;i++){
        cout<<t[i]<<" ";
    }
    return 0;
}
